
<h3>Registro de Empresas</h3>
<br>
<%= form_with model: @empresas do |form| %>
    <div>
        <%= form.label :Razon_Social %>
        <%= form.text_field :razon_social %>
    </div>

    <div>
        <%= form.label :RUT %>
        <%= form.text_field :rut %>
    </div>

    <div>
        <%= form.label :Representante %>
        <%= form.text_field :representante %>
    </div>
    
    <div>
        <%= form.label :Direccion %>
        <%= form.text_field :direccion %>
    </div>
    
    <div>
        <%= form.label :Email %>
        <%= form.email_field :email %>
    </div>

    <div>
        <%= form.label :Telefono %>
        <%= form.text_field :telefono %>
    </div>
    
    <div>
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>
    
    <div>
        <%= form.submit "Registrar Empresa" %>
    </div>
<% end %>

