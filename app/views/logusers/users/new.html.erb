
<h3>Registro de Usuario</h3>
<br>
<%= form_with model: @users do |form| %>
    <div>
        <%= form.label :RUT %>
        <%= form.text_field :rut %>
    </div>
    
    <div>
        <%= form.label :Nombre_Apellido %>
        <%= form.text_field :nombre %>
    </div>
    
    <div>
        <%= form.label :Fecha_Nacimiento %>
        <%= form.date_field :fecha_nac %>
    </div>
    
    <div>
        <%= form.label :Sexo %>
        <%= form.text_field :sexo %>
    </div>
    
    <div>
        <%= form.label :Telefono %>
        <%= form.text_field :telefono %>
    </div>
    
    <div>
        <%= form.label :Email %>
        <%= form.email_field :email %>
    </div>

    <div>
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>
    
    <div>
        <%= form.submit "Guardar Regitro" %>
    </div>
<% end %>